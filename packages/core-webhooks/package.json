{
	"name": "@arkecosystem/core-webhooks",
	"version": "3.1.1",
	"description": "Webhooks for ARK Core",
	"license": "MIT",
	"contributors": [
		"Brian Faust <brian@ark.io>"
	],
	"main": "dist/index",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pnpm run clean && pnpm run compile",
		"build:docs": "../../node_modules/typedoc/bin/typedoc --out docs src",
		"build:watch": "pnpm run clean && pnpm run compile -- -w",
		"clean": "rimraf dist",
		"compile": "node ../../node_modules/typescript/bin/tsc",
		"prepublishOnly": "pnpm run build",
		"pretest": "bash ../../scripts/pre-test.sh",
		"test": "cross-env CORE_ENV=test jest --forceExit"
	},
	"dependencies": {
		"@arkecosystem/core-kernel": "workspace:*",
		"@arkecosystem/crypto": "workspace:*",
		"@hapi/boom": "9.0.0",
		"@hapi/hapi": "20.1.5",
		"fs-extra": "8.1.0",
		"joi": "17.4.2",
		"lowdb": "1.0.0",
		"uuid": "8.3.2"
	},
	"devDependencies": {
		"@types/fs-extra": "8.1.2",
		"@types/hapi__boom": "7.4.1",
		"@types/hapi__joi": "17.1.7",
		"@types/uuid": "8.3.1"
	},
	"engines": {
		"node": ">=10.x"
	},
	"publishConfig": {
		"access": "public"
	}
}
