{
    "name": "@arkecosystem/core-snapshots",
    "version": "3.1.1",
    "description": "Provides live local streamed snapshots functionality for ARK Core",
    "license": "MIT",
    "contributors": [
        "Sebastijan Ku≈æner <sebastijan@ark.io>"
    ],
    "main": "dist/index",
    "types": "dist/index",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm run clean && tsc",
        "build:watch": "pnpm run clean && tsc -w",
        "clean": "rimraf dist",
        "test": "cross-env CORE_ENV=test jest --forceExit"
    },
    "dependencies": {
        "@arkecosystem/core-database": "workspace:*",
        "@arkecosystem/core-kernel": "workspace:*",
        "@arkecosystem/crypto": "workspace:*",
        "bytebuffer": "5.0.1",
        "fs-extra": "8.1.0",
        "joi": "17.4.2",
        "msgpack-lite": "0.1.26",
        "ora": "4.1.1",
        "pg-query-stream": "3.4.2",
        "pluralize": "8.0.0",
        "typeorm": "0.2.25",
        "xcase": "2.0.1"
    },
    "devDependencies": {
        "@arkecosystem/utils": "1.3.1",
        "@types/fs-extra": "8.1.2"
    },
    "engines": {
        "node": ">=16.x"
    },
    "arkecosystem": {
        "core": {
            "alias": "snapshots"
        }
    }
}
